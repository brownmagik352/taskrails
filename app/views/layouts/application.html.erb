<!DOCTYPE html>
<html>
<head>
  <title>Taskrails</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= render 'layouts/header' %>

<% if flash[:notice] %>
	<div id="notice"><%= flash[:notice] %> </div>

<% end %> 

<div id="content">
	<%= yield %>
</div>

</body>
</html>
